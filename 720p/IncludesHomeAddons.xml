	<include name="HomeAddonsAnimation">
		<animation effect="slide" start="0,0" end="0,-205" time="0" condition="Skin.HasSetting(lowermenu)">Conditional</animation>
		<animation type="Visible" condition="!Skin.HasSetting(noslideaddon)">
			<effect type="slide" start="0,30" end="0,0" delay="200" easing="out" tween="quadratic" time="200"/>
			<effect type="fade" start="0" end="100" delay="200" time="200"/>
		</animation>
		<animation type="Hidden" condition="!Skin.HasSetting(noslideaddon)">
			<effect type="zoom" start="100" end="70" center="auto" easing="in" tween="quadratic" time="300" />
			<effect type="fade" start="100" end="0" time="300" />
		</animation>
		<animation effect="fade" start="100" end="0" time="200" condition="!ControlGroup(9002).HasFocus + Skin.HasSetting(noslideaddon)">conditional</animation>
		<animation type="conditional" condition="ControlGroup(9002).HasFocus + Skin.HasSetting(noslideaddon)">
			<effect type="slide" start="0,30" end="0,0" delay="200" easing="out" tween="quadratic" time="200" />
			<effect type="fade" start="0" end="100" delay="200" time="200" />
		</animation>
		<animation type="Hidden" condition="ControlGroup(9002).HasFocus + Skin.HasSetting(noslideaddon)">
			<effect type="zoom" start="100" end="70" center="auto" easing="in" tween="quadratic" time="300"/>
			<effect type="fade" start="100" end="0" time="300"/>
		</animation>
		<hitrect x="0" y="530" w="1280" h="120"/>
	</include>
